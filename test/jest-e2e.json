{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": ["node_modules", "<rootDir>"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@src/(.*)$": "<rootDir>/../src/$1",
    "^@modules/(.*)$": "<rootDir>/../src/modules/$1",
    "^@config/(.*)$": "<rootDir>/../src/config/$1"
  },
  "collectCoverage": false
}
